<?xml version="1.0" encoding="utf8"?>
<presentation>

	<setup>
		<slideWidth>400</slideWidth>
		<slideHeight>200</slideHeight>
		
		<debug>
			<showTitles>true</showTitles>
		</debug>
	</setup>
	
	<slides>
		
		<slide>
			<title>Click red square to open google.com\nClick outside red square for dnnsharp.com</title>
			<background>
				<bgType>solid</bgType>
				<color>0xF8F091</color>
			</background>

			<link>
				<url>http://www.dnnsharp.com</url>
				<target>_blank</target>
			</link>
					
			<objects>
				<object>
					<objType>graphic</objType>
					<src>../../.res/square-red.png</src>
					<link>
						<url>http://www.google.com</url>
						<target>_blank</target>
					</link>
				</object>
			</objects>
		</slide>
	</slides>
</presentation>
