<?xml version="1.0" encoding="utf8"?>
<presentation>

	<setup>
		<slideWidth>400</slideWidth>
		<slideHeight>200</slideHeight>

		<debug>
			<showTitles>true</showTitles>
		</debug>
	</setup>
	
	<slides>
		
		<slide>
			<title>Text displayed with standard font Verdana</title>
			<background>
				<bgType>solid</bgType>
				<color>0xCFE7FB</color>
			</background>
			<objects>
				<object>
					<objType>text</objType>
					<content>The quick brown fox jumps over the lazy dog</content>
					<defaultStyles>font-family: Verdana;</defaultStyles>
				</object>
			</objects>
		</slide>
		
	</slides>
</presentation>
